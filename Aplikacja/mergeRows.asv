function [A] = mergeRows(A, row2)
counter = 1;
for j = 1 : 6
        for i = 1 : 6
            if (A(j, i) == 0)
                if (~(A(row2, counter) == 0))
                A(j, i) = A(row2, counter)
                counter = counter + 1;
                else
                    break
                end
            end
        end
end
A(row2, :) = 0
end